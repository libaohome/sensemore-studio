{
    "java.compile.nullAnalysis.mode": "automatic",
    "java.configuration.updateBuildConfiguration": "automatic",
    "python.testing.pytestEnabled": true,
    "[python]": {
        "editor.formatOnSave": true,
        "editor.tabSize": 4
    },
    "files.exclude": {
        "**/__pycache__": true,
        "**/*.pyc": true
    }
}